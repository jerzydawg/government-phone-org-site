{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.government-phone.org"
        }
      ],
      "destination": "https://government-phone.org/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<state>[a-z]{2})\\.government-phone\\.org"
        }
      ],
      "destination": "https://government-phone.org/:state/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<city>[a-z0-9-]+)-(?<state>[a-z]{2})\\.government-phone\\.org"
        }
      ],
      "destination": "https://government-phone.org/:state/:city/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "(?<city>[a-z0-9-]+)-(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/:city/"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<city>[a-z0-9-]+)-(?<state>[a-z]{2})\\.(?<domain>.*)"
        }
      ],
      "destination": "/:state/:city/"
    }
  ]
}